<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.soundsignatureintegration"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-configuration
        android:reqFiveWayNav="true"
        android:reqHardKeyboard="true"
        android:reqKeyboardType="twelvekey"
        android:reqNavigation="trackball"
        android:reqTouchScreen="finger" />
    <application
        
       android:icon="@drawable/ic_launcher"
       android:label="@string/app_name"
       
     >       
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" >
        </activity>
        <activity android:name=".SelectImageActivity" >
        </activity>
        <activity android:name=".FilePickerActivity" >
        </activity>
        <activity
            android:name=".ReadFileActivity"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".ShowImageActivity"
            android:noHistory="true" >
        </activity>
    
    </application>

</manifest>